   journal = {International Information and Library Review},
   keywords = {Cognitive skills,information literacy,media and information literacy,policies in higher education},
   pages = {354-371},
   publisher = {Taylor and Francis Ltd.},
   title = {The Role of Policies on Teaching Information Literacy in Higher Education: A Comparative Study in Italy and Thailand},
