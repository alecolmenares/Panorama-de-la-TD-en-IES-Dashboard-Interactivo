@article{dfsdf,
   abstract = {Digital technology offers an opportunity for increased productivity in public organizations, but organizations struggle to gain revenue from their investments, known as the productivity paradox. This study examines the role and relations of digital adoption (DA), deliberate learning (DL), and managerial dynamic capability (MDC) on improving productivity in a digital transformation (DT) process in the Norwegian courts. The results show that DL had a strong impact on productivity and that MDC plays an important role in enabling DL processes.},
   author = {Linn Slettum Bjerke-Busch and Sebastian Thorp},
   doi = {10.1080/14719037.2023.2225510},
   issn = {14719045},
   journal = {Public Management Review},
   keywords = {Public sector,deliberate learning,digital adoption,digital transformation,managerial dynamic capability},
   publisher = {Taylor and Francis Ltd.},
   title = {Overcoming the Productivity Paradox in the Public Sector by Managing Deliberate Learning},
   year = {2023},
}
