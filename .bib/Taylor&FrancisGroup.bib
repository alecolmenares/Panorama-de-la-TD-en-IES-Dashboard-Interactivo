   doi = {10.1080/10580530.2022.2130480},
   issn = {10580530},
   issue = {3},
   journal = {Information Systems Management},
   keywords = {Boundary spanning,IS outsourcing,digital innovation,innovation outsourcing,institutional environment,institutional logics},
   pages = {250-269},
   publisher = {Taylor and Francis Ltd.},
   title = {Boundaries and Boundary Spanning in Digital Innovation Outsourcing: The Influence of Institutional Logics and Governance Systems},
   volume = {40},
   year = {2023},
}
