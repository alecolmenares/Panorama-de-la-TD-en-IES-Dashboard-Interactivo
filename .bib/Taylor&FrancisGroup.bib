   keywords = {Arab journalism,Investigative journalism,culture of journalism,data journalism,digital journalism,innovative journalism},
   pages = {1400-1415},
   publisher = {Routledge},
   title = {Conceptualising Innovation Through a Cultural Model: Arab Investigative Journalism},
   volume = {22},
   year = {2021},
}
