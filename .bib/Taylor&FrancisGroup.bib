@article{Blanco2020,
   abstract = {This article examines the convergence of the R&D expenditure in the EU28 for 2004–2015. We initially run a sigma convergence analysis and the results show convergence in the total expenditure, due to the behaviour of the business and higher education sectors, despite government sector divergence. However, noticeable differences between the EU15 and 13 EU countries are apparent. The business enterprise sector is the main driver of EU15 R&D convergence, whereas for the EU13 this role is played by the government expenditure. In addition, the economic crisis has impacted differently on both groups. The club convergence approach allows us to explore these insights through individualized analysis and clusterization. Results for the EU28 show two clubs for the total expenditure, but the analysis of its components reveals a larger grouping. Our results evidence the necessity of revising the EU R&D policies towards greater coordination and resources, and the implementation of new instruments, due to the impact of this expenditure on growth, development and integration.},
   author = {Francisco A. Blanco and Francisco J. Delgado and Maria J. Presno},
   doi = {10.1080/1331677X.2020.1756371},
   issn = {1331677X},
   issue = {1},
   journal = {Economic Research-Ekonomska Istrazivanja },
   keywords = {Convergence,European Union,H5,O3,O4,R&D,expenditure},
   month = {1},
   pages = {1685-1710},
   publisher = {Taylor and Francis Ltd.},
   title = {R&D expenditure in the EU: convergence or divergence?},
   volume = {33},
   year = {2020},
}
@article{dfsdf,
   abstract = {Today's political and economic framework is marked significantly by globalisation and AI automation exponential expansion. Their advancements ensure development and prosperity–which is often not equally distributed across nations. A study showed that U.S. citizens experienced a decrease in the medium household income although their productivity increased, making the problem of distribution visible. Why is this digital era disbalancing the key drivers that used to grow in union? A possible explanation is presented in this article through variables as total factor productivity and Globalisation Index. Investment in human capital, in the form of education, is discussed.},
   author = {Mieta Bobanović},
   doi = {10.1080/1331677X.2021.2012217},
   issn = {1331677X},
   issue = {1},
   journal = {Economic Research-Ekonomska Istrazivanja },
   keywords = {Globalisation Index,economic growth,education,human capital,technology,total factor productivity},
   pages = {4149-4158},
   publisher = {Taylor and Francis Ltd.},
   title = {The shadows of economic growth: AI automation and globalisation},
   volume = {35},
   year = {2022},
}
@article{Boehmer2020,
   abstract = {This research paper explores the emerging potential of IoT technology as an enabler for manufacturers seeking to exploit opportunities for new production, business and operating models. Following an analysis of extant literature and exploration of four in-depth cases, the paper presents four dominant pathways to servitizing the business model through IoT implementation. This first finding is extended in the cross-case analysis, through a categorization of cases into the four pathways, comparing different levels of supplier integration and information exchange. Using this data and categorizations, the paper arrives at certain theoretical propositions regarding the wider impact of IoT technology implementation on information exchange and relational rents through self-enforcing safeguards, risk and financial incentive sharing and lastly transaction cost economics. These propositions lead to the recommendation for suppliers to adopt a servitization pathway of ‘operational service’ models, in order to reap maximum competitive benefit and return on specific investments. This suggests a dependence on the servitization pathway chosen by the supplier, implying that there is no single solution to deal with buyer-supplier relationships in IoT servitization environments.},
   author = {Julius H. Boehmer and Manish Shukla and Dharm Kapletia and Manoj Kumar Tiwari},
   doi = {10.1080/09537287.2019.1631465},
   issn = {13665871},
   issue = {2-3},
   journal = {Production Planning and Control},
   keywords = {IoT,Servitization,buyer-supplier relationships,manufacturing,services},
   month = {2},
   pages = {203-219},
   publisher = {Taylor and Francis Ltd.},
   title = {The impact of the Internet of Things (IoT) on servitization: an exploration of changing supply relationships},
   volume = {31},
   year = {2020},
}
