@article{Alekhina2023,
   abstract = {Over the past decades ASEAN countries have experienced rapid economic growth accompanied by a dramatic fall in poverty rates, but income inequality has not retreated. This research aims at identifying factors which could contribute to more equally distributed growth in ASEAN. To measure inclusive growth, we use a variable integrating per capita income growth and an equity index. A cross-country panel analysis of the impact of macro-structural factors on inclusive growth and its two components suggests that fiscal redistribution, female labour force participation, productivity growth, FDI inflows, digitalization, and savings significantly drive inclusive growth. A scenario analysis based on our econometric results suggests that the implementation of fiscal redistribution and labour market-oriented structural reforms could help significantly accelerate inclusive growth in ASEAN.},
   author = {Victoriia Alekhina and Giovanni Ganelli},
   doi = {10.1080/13547860.2021.1981044},
   issn = {14699648},
   issue = {3},
   journal = {Journal of the Asia Pacific Economy},
   keywords = {Inclusive growth,equity,income distribution,inequality},
   pages = {1196-1228},
   publisher = {Routledge},
   title = {Determinants of inclusive growth in ASEAN},
   volume = {28},
   year = {2023},
}
