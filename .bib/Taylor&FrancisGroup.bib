@article{Yang2023,
   abstract = {The significance of new technologies and digitalization have been recognized in maritime logistics operations amid COVID pandemic. Understanding the adoption of digitalization and collaborative digital platform between participants in industry is important, particularly the path of digital transformation and the impacts they create. However, these issues have not been fully explored in the maritime logistics industry. This study thus developed a model that incorporates digitalization, digital logistics platform adoption, organization’s digital transformation, as well as organizational performance, and examined the associations between them through six research hypotheses. Postal questionnaires were distributed to shipping companies, port operators, shipping agencies, and ocean freight forwarders in Taiwan. The survey yielded a total of 165 valid responses. Subsequently, CFA was applied to verify the model, and SEM was employed to examine the proposed hypotheses. The main findings of the study indicate that enhancing digitalization and embracing digital logistics platforms can significantly contribute to digital transformation. Both digitalization and digital transformation have a positive impact on organizational performance. However, the adoption of digital logistics platforms currently demonstrates an insignificant effect on organizational performance at this stage. Consequently, leveraging the lens of network effects theory, the findings are further discussed, and implications are suggested.},
   author = {Chung Shan Yang and Moses Shang Min Lin},
   doi = {10.1080/03088839.2023.2234911},
   issn = {14645254},
   journal = {Maritime Policy and Management},
   keywords = {Maritime logistics,digital logistics platform,digital transformation,network effects,organizational performance,structural equation modeling (SEM)},
   publisher = {Routledge},
   title = {The impact of digitalization and digital logistics platform adoption on organizational performance in maritime logistics of taiwan},
   year = {2023},
}
@article{Yang2022,
   abstract = {The relevant information extracted from financial documents can be useful in making investment-related decisions. However, the unstructured form of financial data and the sheer volume render manual analysis erroneous and tedious. Internet information is a valuable key for providing insights into the financial records retrieved from the homepage of the financial organization. The extraction of information is a prerequisite for competitive financial information. Information retrieval techniques retrieve the most pertinent data and extract financial data automatically. This paper describes a generic approach to developing an information extraction system that uses a deep learning model. To aid investment decisions, the extraction of financial documents is necessary. The paper compares the results of the improved HMM model and the deep learning approach for the extraction of highly accurate financial information. The simulation results are compared with the precision rate (PR) and recall rate (RR). The deep learning model realizes relatively precise extraction of features for company homepage financial information compared to the HMM model. The deep learning approach achieves an accuracy of 78.71%.},
   author = {Ping Yang},
   doi = {10.1080/03772063.2022.2054873},
   issn = {0974780X},
   journal = {IETE Journal of Research},
   keywords = {Blockchain,Deep-learning,Feature extraction,HMM,Information extraction},
   publisher = {Taylor and Francis Ltd.},
   title = {Financial Information Extraction Using the Improved Hidden Markov Model and Deep Learning},
   year = {2022},
}
