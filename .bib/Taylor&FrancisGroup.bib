   journal = {Journal of Sustainable Finance and Investment},
   keywords = {CSR ratings,Sustainable Development Goals,Sustainable Development Investing,equity indices},
   pages = {702-722},
   publisher = {Taylor and Francis Ltd.},
   title = {Index construction for sustainable development investing},
   volume = {13},
   year = {2023},
}
@article{Bastidas2023,
   abstract = {The concept of Enterprise Architecture is suitable for managing the complexity of heterogeneous systems and technologies in Smart Cities. However, many cities and their urban governance processes still face the challenges of digitalizing public services. This paper aims to assess the applicability of TOGAF in a real-world Smart City following a case study method. A novel research agenda with ten major directions is provided as a result of the practical observations. This contributes to the current understanding of the application of Enterprise Architecture in Smart Cities, focusing on the identified issues in practice and the need for further research.},
   author = {Viviana Bastidas and Marija Bezbradica and Mihai Bilauca and Michael Healy and Markus Helfert},
