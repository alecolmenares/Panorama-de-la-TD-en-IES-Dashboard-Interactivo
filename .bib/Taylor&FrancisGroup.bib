   title = {Serial entrepreneurs: A review of literature and guidance for future research},
   volume = {61},
   year = {2023},
}
@article{dfsdf,
   abstract = {In recent years, we have encountered new gains in information technology applications in providing organizational services. Achieving electronic readiness is the first step for success in competition with other organizations. This study was designed to introduce and implement an applicable model for assessing electronic readiness on the organizational level. Based on literature review, this study has identified effective dimensions and indicators for organizational e-readiness assessment and it has proposed the primary framework based on 32 indicators and six factors. The research design of the study was a descriptive one and the target population of this study was the workers of Jihad-e Keshavarzi (Agricultural Organization) in Ardebil Province, Iran. The instrument used in this study was a questionnaire whose validity was confirmed by the experts and the estimated reliability, using Cronbach's alpha, was.976. Factorial structure of the proposed framework was confirmed by confirmatory factor analysis techniques. Finally, e-readiness with its dimensions and indicators was measured.},
   author = {Zahra Daghighi Masouleh and Mohammad Sadegh Allahyari and Reza Ebrahimi Atani},
   doi = {10.1080/10496505.2014.957761},
   issn = {15404722},
   issue = {4},
   journal = {Journal of Agricultural and Food Information},
   month = {10},
   pages = {295-310},
   publisher = {Bellwether Publishing, Ltd.},
