   journal = {China Economic Journal},
   keywords = {Digital financial platforms,bigtech lending,digital financial infrastructure,regulatory framework},
   pages = {216-230},
   publisher = {Routledge},
   title = {Innovative development and regulation of digital financial platforms in China},
   volume = {15},
   year = {2022},
}
