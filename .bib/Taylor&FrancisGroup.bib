   keywords = {Assessment,OSCE,Ottawa consensus,assessment of competence,systems of assessment},
   pages = {978-983},
   pmid = {36786837},
   publisher = {Taylor and Francis Ltd.},
   title = {Synthesis and perspectives from the Ottawa 2022 conference on the assessment of competence},
   volume = {45},
   year = {2023},
}
