@article{dfsdf,
   abstract = {Product innovation within institutional journalism has become an increasingly apparent strategy for news organizations to form and maintain audience interest and economic stability. The common desire for product innovation to be a fiscal savior ultimately challenges internal boundaries of professions. This is particularly apparent as the product innovation field within the news industry expands and blurs institutional logics. To explore the ways product personnel challenge and contribute to the shifting boundaries of journalism, this research draws on content analysis data from top national U.S. print newspaper organizations’ LinkedIn member profiles. The research provides a more nuanced understanding of the shifting and conflicting logics of journalism through laborer backgrounds and experiences regarding product and innovation. It applies cultural entrepreneurship and conflicting institutional logic concepts to argue these news workers introduce knowledge skillsets and are reshaping the profession of journalism.},
   author = {Rowan McMullen Cheng and Valérie Bélair-Gagnon},
   doi = {10.1080/1461670X.2022.2102534},
   issn = {14699699},
   issue = {12},
   journal = {Journalism Studies},
   keywords = {Product,conflicting institutional logics,cultural entrepreneurship,journalism,labor,skills},
   pages = {1531-1547},
   publisher = {Routledge},
   title = {Journalistic Product Personnel as Cultural Entrepreneurs: An Exploration of Background, Tenureship, and Knowledge Skills},
   volume = {23},
   year = {2022},
}
